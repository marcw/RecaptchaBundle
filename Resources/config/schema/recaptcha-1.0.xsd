<?xml version="1.0" encoding="UTF-8" ?>

<xsd:schema xmlns="http://symfony.com/schema/dic/ewz/recaptcha"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    targetNamespace="http://symfony.com/schema/dic/ewz/recaptcha"
    elementFormDefault="qualified">

    <xsd:element name="config" type="config" />

    <xsd:complexType name="config">
        <xsd:attribute name="pubkey" type="xsd:string" />
        <xsd:attribute name="privkey" type="xsd:string" />
        <xsd:attribute name="secure" type="xsd:boolean" />
    </xsd:complexType>
</xsd:schema>
